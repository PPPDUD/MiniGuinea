{
  "manifest_version": 3,
  "name": "MiniGuinea",
  "version": "0.8.0",
  "description": "Eats ads and trackers so that you don't have to.",
  "permissions": [
    "declarativeNetRequest"
  ],

  "icons": {
   "16": "16.png",
   "32": "32.png",
   "48": "48.png",
   "128": "128.png"
 },

 "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset1",
      "enabled": true,
      "path": "ruleset1.json"
    }]},

  "host_permissions": [
    "<all_urls>"
  ]
}
